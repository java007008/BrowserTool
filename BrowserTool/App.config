<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
    <appSettings>
        <!-- 是否启用自动签到 -->
        <add key="CheckInEnabled" value="true" />
        <!-- 早上签到时间 -->
        <add key="MorningCheckInTime" value="09:00" />
        <!-- 晚上签到时间 -->
        <add key="EveningCheckInTime" value="18:00" />
        <!-- 随机分钟数 -->
        <add key="RandomMinutes" value="30" />
        <!-- 是否启用鼠标活动模拟 -->
        <add key="EnableMouseActivitySimulation" value="true" />
        <!-- 鼠标活动间隔（分钟） -->
        <add key="MouseActivityInterval" value="5" />
        <!-- 鼠标移动步长 -->
        <add key="MouseMoveStep" value="10" />
        <!-- 测试环境URL -->
        <add key="TestEnvironmentUrl" value="http://test.example.com" />
        <!-- 是否显示手动签到按钮 -->
        <add key="ShowManualCheckInButton" value="true" />
        <!-- 需要匹配的域名列表，用逗号分隔 -->
        <add key="CheckInDomains" value="example.com,test.example.com,dev.example.com" />
    </appSettings>
</configuration>