<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- 是否启用自动打卡功能 -->
    <add key="CheckInEnabled" value="true" />
    
    <!-- 是否显示手动打卡按钮 -->
    <add key="ShowManualCheckInButton" value="false" />
    
    <!-- 早上打卡时间 -->
    <add key="MorningCheckInTime" value="08:50:00" />
    
    <!-- 晚上打卡时间 -->
    <add key="EveningCheckInTime" value="18:03:00" />
    
    <!-- 随机延迟时间（分钟） -->
    <add key="RandomMinutes" value="10" />
    
    <!-- 是否启用鼠标活动模拟 -->
    <add key="EnableMouseActivitySimulation" value="true" />
    
    <!-- 鼠标活动间隔时间（毫秒） -->
    <add key="MouseActivityInterval" value="120000" />
    
    <!-- 鼠标移动步长（像素） -->
    <add key="MouseMoveStep" value="1" />
    
    <!-- 测试环境URL -->
    <add key="TestEnvironmentUrl" value="http://www.google.com" />
  </appSettings>
</configuration>